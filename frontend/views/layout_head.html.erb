<link href="<%= "#{AppConfig[:frontend_proxy_prefix]}assets/styles/style.css" %>" media="all" rel="stylesheet" type="text/css">
<link href="<%= "#{AppConfig[:frontend_proxy_prefix]}assets/styles/conversation.css" %>" media="all" rel="stylesheet" type="text/css">
<link href="<%= "#{AppConfig[:frontend_proxy_prefix]}assets/styles/quote_editor.css" %>" media="all" rel="stylesheet" type="text/css">

<script type="text/template" id="MAPBrowseActions">
  <li class="divider"></li>
  <li class="dropdown-submenu">
    <a href="javascript:void(0);"><%= I18n.t('browse_menu.agency_services') %></a>
    <ul class="dropdown-menu">
      <li><%= link_to I18n.t("transfer_proposal._plural"), :controller => :transfer_proposals, :action => :index %></li>
      <li><%= link_to I18n.t("transfer._plural"), :controller => :transfers, :action => :index %></li>
      <li class="divider"></li>
      <li><%= link_to I18n.t("file_issue_request._plural"), :controller => :file_issue_requests, :action => :index %></li>
      <li><%= link_to I18n.t("file_issue._plural"), :controller => :file_issues, :action => :index %></li>
      <li class="divider"></li>
      <li><%= link_to I18n.t("search_request._plural"), :controller => :search_requests, :action => :index %></li>
    </ul>
  </li>
</script>

<script>
    $(function() {
        $('.repository-header .navbar-nav .browse-container .dropdown-menu .divider:first').before(AS.renderTemplate("MAPBrowseActions"))
    });
</script>
